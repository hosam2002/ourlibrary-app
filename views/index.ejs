                <div class="blog" id="blog-<%= latest._id%>">

                    <div class="blog__wrapper">

                        <div class="blog__share">
                        
                            <span class="blog__clipboard" data-share="%domain%/blogs/#<%= latest._id %>" id="share-link">
                                
