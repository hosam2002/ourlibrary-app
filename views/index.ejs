                        <div class="blog__date"> <%= blogDate(blogFind.length - 1) %> </div>
                        
                    </div>

                    <% if (blogCount > 1) { %>

                        <div class="blog__button">

                            <a href="/blogs" class="blog__link"> المزيـد </a>
    
